{
	"name": "seva-sahayog-monorepo",
	"private": true,
	"version": "1.0.0",
	"description": "Seva Sahayog Donation Matching Portal",
	"packageManager": "yarn@4.5.1",
	"workspaces": [
		"frontend",
		"server",
		"client",
		"backend"
	],
	"scripts": {
		"dev": "yarn --cwd frontend dev",
		"build": "yarn --cwd frontend build",
		"preview": "yarn --cwd frontend preview",
		"dev:server": "yarn --cwd server dev",
		"start:server": "yarn --cwd server start",
		"seed": "yarn --cwd server seed"
	}
}
